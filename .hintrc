{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust"
        ]
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "no-inline-styles": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 106",
    "not firefox <= 106",
    "not ios_saf <= 14.8",
    "not opera <= 91",
    "not safari <= 14.1"
  ]
}